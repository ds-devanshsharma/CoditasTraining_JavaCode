<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context = "http://www.springframework.org/schema/context"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config></context:annotation-config>
    <bean id="address" class="com.bean.Address"  >
    <constructor-arg value="Pune"/>
    <constructor-arg value="MH"/>
    </bean>

    <bean id="student" class="com.bean.Student" >
        <constructor-arg name="id" value="11056"/>
        <constructor-arg name="name" value="Devansh"/>
    </bean>
    <bean id="employee" class="com.bean.Employee" >
        <property name="id" value="1111"/>
        <property name="name" value="Aarti"/>
    </bean>
</beans>